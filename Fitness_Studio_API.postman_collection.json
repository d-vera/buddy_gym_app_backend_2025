              "host": ["{{base_url}}"],
              "path": ["exercises", "1", ""]
            }
          }
        },
        {
          "name": "Filter Exercises by Muscle",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/exercises/?muscle=1",
              "host": ["{{base_url}}"],
              "path": ["exercises", ""],
              "query": [
                {
                  "key": "muscle",
                  "value": "1"
                }
              ]
            }
          }
        }
      ]
    },
